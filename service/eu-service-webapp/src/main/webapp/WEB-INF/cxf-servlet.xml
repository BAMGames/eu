<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jaxws="http://cxf.apache.org/jaxws"
       xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">
    <import resource="classpath:META-INF/cxf/cxf.xml"/>
    <import resource="classpath:META-INF/cxf/cxf-servlet.xml"/>
    <jaxws:endpoint id="gameService"
                    implementor="com.mkl.eu.service.service.ws.impl.GameWsServiceImpl"
                    address="/GameService"/>
    <jaxws:endpoint id="boardService"
                    implementor="com.mkl.eu.service.service.ws.impl.BoardWsServiceImpl"
                    address="/BoardService"/>
    <jaxws:endpoint id="chatService"
                    implementor="com.mkl.eu.service.service.ws.impl.ChatWsServiceImpl"
                    address="/ChatService"/>
    <jaxws:endpoint id="economicService"
                    implementor="com.mkl.eu.service.service.ws.impl.EconomicWsServiceImpl"
                    address="/EconomicService"/>
    <jaxws:endpoint id="battleService"
                    implementor="com.mkl.eu.service.service.ws.impl.BattleWsServiceImpl"
                    address="/BattleService"/>
    <jaxws:endpoint id="siegeService"
                    implementor="com.mkl.eu.service.service.ws.impl.SiegeWsServiceImpl"
                    address="/SiegeService"/>
    <jaxws:endpoint id="interPhaseService"
                    implementor="com.mkl.eu.service.service.ws.impl.InterPhaseWsServiceImpl"
                    address="/InterPhaseService"/>
    <jaxws:endpoint id="tablesService"
                    implementor="com.mkl.eu.service.service.ws.impl.TablesWsServiceImpl"
                    address="/TablesService"/>
    <jaxws:endpoint id="testService"
                    implementor="com.mkl.eu.service.service.ws.impl.TestWsServiceImpl"
                    address="/TestService"/>
</beans>